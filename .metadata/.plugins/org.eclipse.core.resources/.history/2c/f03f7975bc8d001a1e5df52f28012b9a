package interfaces;
interface A
{
	public void Adisplay();
	
}
class AB implements A
{
	public void Adisplay()
	{
		System.out.println("we are inside class A");
	}
}

public class Amain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		AB obj = new AB();
		obj.Adisplay();

	}

}
